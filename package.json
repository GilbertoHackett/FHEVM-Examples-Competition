{
  "name": "fhevm-examples",
  "version": "1.0.0",
  "description": "Comprehensive FHEVM Examples Competition Submission - Complete ecosystem for building, testing, and deploying privacy-preserving applications using Fully Homomorphic Encryption",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:watch": "hardhat test --watch",
    "clean": "hardhat clean",
    "deploy": "hardhat run scripts/deploy.ts",
    "create-example": "ts-node scripts/create-fhevm-example.ts",
    "create-category": "ts-node scripts/create-fhevm-category.ts",
    "generate-docs": "ts-node scripts/generate-docs.ts",
    "validate-examples": "ts-node scripts/validate-examples.ts",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "format": "prettier --write .",
    "check": "npm run compile && npm run lint && npm run test && npm run test:coverage",
    "dev": "hardhat node",
    "console": "hardhat console",
    "flatten": "hardhat flatten",
    "docs:api": "typedoc --out docs src",
    "benchmark": "hardhat test --performance",
    "audit": "npm audit"
  },
  "keywords": [
    "fhevm",
    "homomorphic-encryption",
    "privacy",
    "smart-contracts",
    "solidity",
    "ethereum",
    "hardhat",
    "examples",
    "encryption",
    "cryptography",
    "privacy-preserving",
    "confidential-computing",
    "zama",
    "fhe"
  ],
  "author": "FHEVM Examples Contributors",
  "license": "BSD-3-Clause-Clear",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fhevm-examples.git"
  },
  "homepage": "https://github.com/yourusername/fhevm-examples#readme",
  "bugs": {
    "url": "https://github.com/yourusername/fhevm-examples/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@typechain/ethers-v5": "^11.0.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.19.0",
    "hardhat-fhevmjs": "^0.4.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.1.3",
    "solhint": "^4.0.1"
  },
  "dependencies": {
    "@fhevm/solidity": "0.9.0",
    "dotenv": "^16.3.1"
  },
  "files": [
    "contracts/",
    "scripts/",
    "test/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
